    "def countNucleotides(filePath):\n",
    "    nucleotide=['A','C','G','T']\n",
    "    with open(filePath) as data:\n",
    "        for line in data:\n",
    "            print(*[line.count(char) for char in nucleotide])\n",
    "            \n",
    "            \n",
    "countNucleotides(\"rosalind_ini.txt\")"
   ]
  }
