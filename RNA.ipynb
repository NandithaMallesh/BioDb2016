    "def DNAtoRNA(filePath):\n",
    "    with open(filePath) as data:\n",
    "        for line in data:\n",
    "            print(\"transcribed RNA:\",line.replace('T','U'))\n",
    "                       \n",
    "            \n",
    "DNAtoRNA(\"rosalind_rna.txt\")"
