    "def countNucleotides(filePath):\n",
    "    nucleotide=['A','C','G','T']\n",
    "    with open(filePath) as data:\n",
    "        for line in data:\n",
    "            print([(char,line.count(char)) for char in nucleotide])\n",
    "            \n",
    "            \n",
    "countNucleotides(\"rosalind_dna.txt\")"
   ]
